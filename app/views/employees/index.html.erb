<div class="container">
  <div class="row">
    <h3>Employees</h3>
    <div class="column">
      <table class="table">
        <tbody>
          <% @employees.each do |employee| %>
            <tr>
              <%= render employee %>
            </tr>
          <% end %>
        </tbody>
      </table>
    </div>
  </div>
</div>
